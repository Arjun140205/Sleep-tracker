{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api/.*).*)",
            "destination": "/client/build/$1"
        },
        {
            "source": "/",
            "destination": "/client/build/index.html"
        }
    ]
}